{
  "extends": [
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "variable-name": false,
    "no-inferrable-types": false,
    "no-any": false
  },
  "linterOptions": {
    "exclude": [
      "**/*.js",
      "node_modules/**"
    ]
  }
}